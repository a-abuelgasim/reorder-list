{
	"name": "reorder-list",
	"version": "1.0.0",
	"description": "Starting point for new projects",
	"main": "src/js/script.js",
	"scripts": {
		"lint:js": "eslint ./src/js --ext .js,.jsx,.ts,.tsx",
		"lint:sass": "stylelint \"src/sass/**/*.scss\"",
		"lint": "npm run lint:sass && npm run lint:js",
		"start": "parcel \"./src/index.html\" --no-cache",
		"test": "jest --watchAll"
	},
	"author": "Ahmed Abuelgasim",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/ahmed-abuelgasim/reorder-list.git"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		]
	},
	"browserslist": [
		"last 2 versions",
		"not IE <= 11"
	],
	"eslintIgnore": [
		"**/*.min.js"
	],
	"devDependencies": {
		"@parcel/transformer-sass": "^2.4.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"eslint-plugin-jest": "^26.1.2",
		"eslint-plugin-no-autofix": "^1.2.3",
		"jest": "^27.5.1",
		"parcel": "^2.4.0",
		"sass": "^1.49.9",
		"stylelint": "^14.6.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.2.0",
		"typescript": "^4.6.2"
	}
}
